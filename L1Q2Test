import static org.junit.Assert.*;
import org.junit.Test;
public class YoungPhysicist {
    @Test
    public void test1() {
        assertTrue(YoungPhysicist.inEqu(0,0,0) == true);
    }
    @Test
    public void test2() {
        assertTrue(YoungPhysicist.inEqu(1,-3,0) == false);
    }
    @Test
    public void test3() {
        assertTrue(YoungPhysicist.inEqu(0,0,5) == false);
    }
}
