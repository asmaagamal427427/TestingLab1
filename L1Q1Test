import static org.junit.Assert.*;
import org.junit.Test;
public class Watermelon {
    @Test
    public void test1() {
        assertTrue(WaterMelon.hasEvenParts(12) == true);
    }
    @Test
    public void test2() {
        assertTrue(WaterMelon.hasEvenParts(6) == true);
    }
    @Test
    public void test3() {
        assertTrue(WaterMelon.hasEvenParts(2) == false);
    }
    @Test
    public void test4() {
        assertTrue(WaterMelon.hasEvenParts(11) == false);
    }
}
