import static org.junit.Assert.*;
import org.junit.Test;
public class Watermelon {
    @Test
    public void test1() {
        assertTrue(WaterMelon.isEvenParts(12) == True);
    }
    @Test
    public void test2() {
        assertTrue(WaterMelon.isEvenParts(6) == True);
    }
    @Test
    public void test3() {
        assertTrue(WaterMelon.isEvenParts(2) == False);
    }
    @Test
    public void test4() {
        assertTrue(WaterMelon.isEvenParts(11) == False);
    }
}
